<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cgi.eincent.salesforce.mapper.PhoneMapper">

    <select id="selectPhoneCatalog" resultType="Phone">
        SELECT 1 as id, 'iphone' as name, 1000.59 as price, 'newest iphone' as description  FROM dual
    </select>

</mapper>